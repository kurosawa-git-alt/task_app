<h2>スケジュール詳細</h2>

<table>
    <tr>
        <th>ID</th>
        <td><%= @schedule.id %></td>
    </tr>
    <tr>
        <th>タイトル</th>
        <td><%= @schedule.title %></td>
    </tr>
    <tr>
        <th>開始日</th>
        <td><%= @schedule.startd %></td>
    </tr>
    <tr>
        <th>終了日</th>
        <td><%= @schedule.endd %></td>
    </tr>
    <tr>
        <th>終日</th>
        <td><%= @schedule.alld %></td>
    </tr>
    <tr>
        <th>メモ</th>
        <td><%= @schedule.memo %></td>
    </tr>
</table>

<div>
    <ul>
        <li><%= link_to "スケジュール一覧に戻る", :schedules %></li>
    </ul>
</div>